{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Bundler",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "lib": [
      "esnext"
    ],
    "types": [
      "node",
      "react",
      "react-native"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "index.js"
  ],
  "extends": "expo/tsconfig.base"
}