{
  "expo": {
    "name": "Lify",
    "slug": "lify-sprint0",
    "scheme": "lify",
    "version": "0.1.0",
    "orientation": "portrait",
    "sdkVersion": "51.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lify.mobile",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Lify a besoin d'accéder à votre galerie pour changer votre photo de profil."
      }
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "apiUrl": "http://localhost:3000"
    }
  }
}